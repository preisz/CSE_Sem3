<?xml version='1.0' encoding='utf-8'?>
<cfsMaterialDataBase xmlns="http://www.cfs++.org/material">
  <material name="PMMA">
    <mechanical>
      <density>
        <linear>
          <real> 1180 </real>
        </linear>
      </density>
      <elasticity>
        <linear>
          <isotropic>
            <elasticityModulus>
              <real> 0.045E+11 </real>
            </elasticityModulus>
            <poissonNumber>
              <real> 0.4 </real>
            </poissonNumber>
          </isotropic>
        </linear>
      </elasticity>
      <damping>
        <rayleigh>
          <!-- Replace Rayleigh Damping Coefficient alpha and beta by your calculated values, it is not necessary to change the measuredFreq-->
          <alpha> 0.167 </alpha>
          <beta> 0.0005 </beta>
          <measuredFreq>0</measuredFreq>
        </rayleigh>
      </damping>
      </mechanical>
  </material>
  
  <material name="PIC 255">
    <mechanical>
      <density>
        <linear>
          <real> 5195.294 </real> <!-- rho eff-->
        </linear>
      </density>
      <elasticity>
        <linear>
          <tensor dim1="6" dim2="6">
            <real>
              <!-- Fill the elasticity tensor with calculated values -->
              <!-- It is the inverse of the compilance tensor whose entries are calculated-->
              6.065E+10 2.855E+10 2.490E+10 0.000E+00 0.000E+00 0.000E+00
              2.855E+10 5.482E+10 1.172E+10 0.000E+00 0.000E+00 0.000E+00
              2.490E+10 1.172E+10 0.000E+00 0.000E+00 0.000E+00 0.000E+00
              0.000E+00 0.000E+00 0.000E+00 3.575E+10 0.000E+00 0.000E+00
              0.000E+00 0.000E+00 0.000E+00 0.000E+00 3.575E+10 0.000E+00
              0.000E+00 0.000E+00 0.000E+00 0.000E+00 0.000E+00 1.406E+10
          </real>
          </tensor>
        </linear>
      </elasticity>
    </mechanical>
    <electric>
      <permittivity>
        <linear>
          <tensor dim1="3" dim2="3">
            <real>
            <!-- Fill the permittivity tensor with calculated values -->
            1.531E-08 0.000E+00 0.000E+00
            0.000E+00 1.531E-08 0.000E+00
            0.000E+00 0.000E+00 1.686E-08
          </real>
          </tensor>
        </linear>
      </permittivity>
    </electric>
    <piezo>
      <piezoCoupling>
        <linear>
          <tensor dim1="3" dim2="6">
            <real>
             <!-- Fill the piezoelectric coupling tensor with calculated values -->
              0.000E+00 0.000E+00 0.000E+00 0.000E+00 1.080E-09 0.000E+00
              0.000E+00 0.000E+00 0.000E+00 1.080E-09 0.000E+00 0.000E+00
              -3.536E-10 -3.536E-10 7.857E-10 0.000E+00 0.000E+00 0.000E+00
        </real>
          </tensor>
        </linear>
      </piezoCoupling>
    </piezo>
  </material>
</cfsMaterialDataBase>
